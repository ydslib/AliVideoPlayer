<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <Button
                    android:id="@+id/dialog"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="弹窗" />

                <Button
                    android:id="@+id/wheelView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="WheelView"/>
                <Button
                    android:id="@+id/alivcWheelDialogFragment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="AlivcWheelDialogFragment"/>
                <Button
                    android:id="@+id/alphaInAnimation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="AlphaInAnimation"/>
                <Button
                    android:id="@+id/aliyunRenderView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="AliyunRenderView"/>
            </LinearLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>